#!/home/s/ops/perl/bin/perl
use strict;
use warnings;

use NS::Util::OptConf;
use NS::TPool::NQRead;

$| ++;

=head1 SYNOPSIS

 $0 --server 127.0.0.1:9999 --group name --code /my/code 
    
=cut

my $option = NS::Util::OptConf->load();
my %o = $option->get( qw( server=s group=s code=s ) )->dump();
$option->assert( qw( server group ) );

NS::TPool::NQRead->new( %o )->run();
